<h2>Creating a  nested objects</h2>
<script>
var employee = {name:"Sagar",age:27, salary:"3.9 lac per annum", address:{address1:"kolhapur",address2:"pune maharashtra",address3:"satara maharashtra"}};

alert(employee.name + ", "+ employee.age +" ,"+ employee.salary+ ", "+employee.address.address1+ " ,"+employee.address.address2+", "+employee.address.address3);
document.write(employee.name + " ,"+ employee.age +" ,"+ employee.salary+ " ,"+employee.address.address1+ " ,"+employee.address.address2+", "+employee.address.address3)
</script>
